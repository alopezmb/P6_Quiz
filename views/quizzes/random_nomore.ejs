<script>
    var audio = new Audio('/sounds/gamewin.mp3');
    audio.play();
</script>
<h1>
    End of Random Play:
</h1>

<div>
    Successful answers = <span id="score"><%= score %></span>
</div>

<p>
    You answered all questions. You are a champion.
</p>

<% if (typeof session.user !=='undefined') { %>
<p>
    <a href="/quizzes/randomplay/scoreupdate/<%= session.user.id %>?score=<%= score %>&_method=PUT">
        <button>Back to Home Page</button></a>
</p>


<% } else { %>
<p>
    <a href="/"><button>Back to Home Page</button></a>
</p>

<% } %>

